{
  "name": "@vuejs-pt/vue-alert",
  "version": "0.3.0",
  "description": "Alert bootstrap style for Vue.js",
  "license": "MIT",
  "repository": "vuejs-pt/vue-alert",
  "author": "David Picarra",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint && jest --coverage",
    "preversion": "npm run test && npm run build",
    "postversion": "git push origin master --follow-tags && npm publish"
  },
  "pre-commit": [
    "test",
    "build"
  ],
  "main": "dist/@vuejs-pt/vue-alert.min.js",
  "keywords": [
    "vue-alert",
    "vue-alert bootstrap"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-vue": "^0.1.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "jest-cli": "^19.0.2",
    "jest-vue-preprocessor": "^0.2.0",
    "node-sass": "^4.5.2",
    "pre-commit": "^1.2.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-vue": "^2.3.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "vue": "^2.2.6"
  }
}
